[Unit]
Description=CatCam Web Application
After=network.target

[Service]
Type=notify
WorkingDirectory=/home/pi/catcam
ExecStart=/home/pi/catcam/CatCam.Web
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=catcam
User=pi
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=ASPNETCORE_URLS=http://0.0.0.0:8080

[Install]
WantedBy=multi-user.target
